<!DOCTYPE html>
<html>
	<head lang="en">
		<title>
			{% block title %}
				{{ page.title }}
			{% endblock %}
			&mdash;
			{{ site.title }}
			&mdash;
			{{ site.subtitle }}
		</title>
		<meta charset="utf-8">
		<meta name="theme-color" content="#ffffff">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		{% block head_meta %}
			<meta name="robots" content="index, follow">
		{% endblock %}
		<link rel="stylesheet" href="{{ site.url }}/assets/build/app.css">

		<link rel="apple-touch-startup-image" href="{{ site.url }}/assets/icons/2048x2048.png">
		<meta name="mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-capable" content="yes">

		<link rel="shortcut icon" sizes="76x76" href="{{ site.url }}/assets/icons/fbanitz/76x76.jpg">
		<link rel="shortcut icon" sizes="120x120" href="{{ site.url }}/assets/icons/fbanitz/120x120.jpg">
		<link rel="shortcut icon" sizes="128x128" href="{{ site.url }}/assets/icons/fbanitz/128x128.jpg">
		<link rel="shortcut icon" sizes="152x152" href="{{ site.url }}/assets/icons/fbanitz/152x152.jpg">

		<link rel="apple-touch-icon" sizes="76x76" href="{{ site.url }}/assets/icons/fbanitz/76x76.jpg">
		<link rel="apple-touch-icon" sizes="120x120" href="{{ site.url }}/assets/icons/fbanitz/120x120.jpg">
		<link rel="apple-touch-icon" sizes="128x128" href="{{ site.url }}/assets/icons/fbanitz/128x128.jpg">
		<link rel="apple-touch-icon" sizes="152x152" href="{{ site.url }}/assets/icons/fbanitz/152x152.jpg">

		<link rel="alternate" type="application/atom+xml" href="{{ site.url }}/atom.xml" title="{{ site.title }} activity feed"/> {% block head_styles %}{% endblock %}{% block head_scripts %}{% endblock %}
	</head>
	<body>
		<header>
			<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
				<div class="container">
					<a class="navbar-brand" href="{{ site.url }}/">{{ site.title }}</a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarCollapse">
						<ul class="navbar-nav mr-auto">
							<li class="nav-item">
								<a class="nav-link" href="{{ site.url }}/blog">Blog</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ site.url }}/blog/categories">Cat√©gories</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="{{ site.url }}/blog/tags">Tags</a>
							</li>
						</ul>
					</div>
				</div>
			</nav>
		</header>
		<main role="main" class="container">
			<div class="row  d-flex justify-content-center">
				<div class="col-sm-8 col-lg-12">
					{% block content_wrapper %}
						{% block content %}{% endblock %}
					{% endblock %}
				</div>
			</div>
		</main>
		<footer class="container text-muted">
			<p>&copy;
				{{ "now"|date("Y") }}
				{{ site.title }}</p>

			<p>Made with love using
				<a href="https://sculpin.io/" target="_blank">Sculpin</a>
			</p>
			<p>
				Check up source on
				<a href="https://github.com/FBanitz/fbanitz.fr">
					<img src="/assets/icons/github/github-mark.svg" style="height: 14px" alt="GitHub logo"/>
					<span>GitHub</span>
				</a>
			</p>
			<p>
				<a href="https://github.com/FBanitz/fbanitz.fr/releases/tag/{{ site.version }}" target="_blank"><span id="version">v{{ site.version }}</span></a>
				updated
				<span id="versionDateTime" class="date">
				{% if site.run_url %}
					<a href="{{ site.run_url }}" target="_blank">
						{{ "now" | date() }}
					</a>
				{% else %}
					{{ "now" | date() }}
				{% endif %}
				UTC</span
			</p>
		</footer>
		{% block scripts %}{% endblock %}
		<script src="https://code.jquery.com/jquery-1.7.1.min.js" integrity="sha256-iBcUE/x23aI6syuqF7EeT/+JFBxjPs5zeFJEXxumwb0=" crossorigin="anonymous"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
		<script src="{{ site.url }}/assets/build/app.js"></script>
		<script src="{{ site.url }}/assets/js/outside_links_blank.js"></script>
		{% block scripts_after %}{% endblock %}
	</body>
</html>
